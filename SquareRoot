class SquareRoot{
    public int mySqrt(int x) {
        if(x == 0) return 0;
        if(x < 4) return 1;
        long root = x;
            while(root*root > x)
                root =(root + x/root)/2; //Newton
        System.out.println(root);
        int root1 = (int)root;
        return root1;
        }

public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("SquareRoot of 8 " + mySqrt(8));
	
    }
}
